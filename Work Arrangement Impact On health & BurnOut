   "cell_type": "code",
   "execution_count": 19,
   "id": "42b9250a",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-07-22T10:17:39.091368Z",
     "iopub.status.busy": "2025-07-22T10:17:39.091040Z",
     "iopub.status.idle": "2025-07-22T10:17:39.338563Z",
     "shell.execute_reply": "2025-07-22T10:17:39.337452Z"
    },
    "papermill": {
     "duration": 0.265914,
     "end_time": "2025-07-22T10:17:39.340283",
     "exception": false,
     "start_time": "2025-07-22T10:17:39.074369",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1000x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Group and reshape\n",
    "grouped = df.groupby([\"Work_Arrangement\", \"Burnout_Level\"])[\"Work_Life_Balance_Score\"].sum().unstack().fillna(0)\n",
    "grouped = grouped.loc[grouped.sum(axis=1).sort_values().index]\n",
    "\n",
    "# Plot\n",
    "ax = grouped.plot(kind=\"barh\", stacked=True, figsize=(10, 6), colormap=\"Set2\")\n",
    "\n",
    "# # Add threshold lines\n",
    "# plt.axvline(x=1000, color='yellow', linestyle='--', linewidth=1.5, label='Threshold = Good')\n",
    "# plt.axvline(x=1500, color='red', linestyle='--', linewidth=1.5, label='Threshold = Bad')\n",
    "\n",
    "# Add data labels\n",
    "for i, (industry, row) in enumerate(grouped.iterrows()):\n",
    "    cumulative = 0\n",
    "    for burnout_level in grouped.columns:\n",
    "        value = row[burnout_level]\n",
    "        if value > 0:\n",
    "            ax.text(cumulative + value / 2, i, f'{int(value)}', va='center', ha='center', fontsize=9, color='black')\n",
    "            cumulative += value\n",
    "\n",
    "# Finalize plot\n",
    "plt.title(\"Work Life Balance Score by Work_Arrangement and Burnout Level\")\n",
    "plt.xlabel(\"Work Life Balance Score\")\n",
    "plt.legend(title=\"Burnout Level\", bbox_to_anchor=(1.05, 1), loc='upper left')\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
  },
