   ],
   "source": [
    "pivot = (\n",
    "    df[df['Balance_Category'] == 'Poor']\n",
    "    .groupby(['Industry', 'Burnout_Level'])\n",
    "    .size()\n",
    "    .unstack(fill_value=0)\n",
    ")\n",
    "\n",
    "pivot['Total'] = pivot.sum(axis=1)\n",
    "pivot['Work-Life Balance'] = pivot['High'] / pivot['Total'] * 100\n",
    "\n",
    "# Plot\n",
    "plt.figure(figsize=(10, 6))\n",
    "sns.heatmap(pivot[['Work-Life Balance']], annot=True, cmap='Blues', fmt=\".1f\")\n",
    "plt.title(\"% Employees with Poor Work-Life Balance (by Industry)\")\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
