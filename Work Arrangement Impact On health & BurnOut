   "source": [
    "# Create the pivot table with sum\n",
    "pivot_table = df.pivot_table(\n",
    "    values=\"Work_Life_Balance_Score\",\n",
    "    index=\"Work_Arrangement\",\n",
    "    aggfunc=\"sum\",\n",
    "    fill_value=0\n",
    ")\n",
    "\n",
    "# Convert to percentage\n",
    "total = pivot_table[\"Work_Life_Balance_Score\"].sum()\n",
    "percent_table = (pivot_table / total) * 100\n",
    "\n",
    "# Plot the heatmap\n",
    "plt.figure(figsize=(8, 5))\n",
    "sns.heatmap(percent_table.values.reshape(-1, 1),\n",
    "            annot=True, cmap=\"Greens\", fmt=\".1f\",\n",
    "            yticklabels=percent_table.index,\n",
    "            xticklabels=[\"% of Total Score\"])\n",
    "plt.title(\"% of Total Work_Life_Balance_Score by Work_Arrangement\")\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
