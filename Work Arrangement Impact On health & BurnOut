   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Group and sort the data\n",
    "industry_scores = df.groupby(\"Work_Arrangement\")[\"Work_Life_Balance_Score\"].sum().sort_values()\n",
    "\n",
    "# Calculate total score for percentages\n",
    "total_score = industry_scores.sum()\n",
    "percentages = (industry_scores / total_score) * 100\n",
    "\n",
    "# Plot the bar chart\n",
    "ax = industry_scores.plot(kind=\"barh\", figsize=(8, 6), color='skyblue')\n",
    "plt.title(\"Work Life Balance Score by Work Arrangement\")\n",
    "plt.xlabel(\"Work Life Balance Score\")\n",
    "plt.tight_layout()\n",
    "\n",
    "# Annotate each bar with percentage\n",
    "for i, (value, percent) in enumerate(zip(industry_scores, percentages)):\n",
    "    ax.text(value + total_score * 0.01, i, f\"{percent:.1f}%\", va='center')\n",
    "\n",
    "plt.show()\n"
   ]
  },
