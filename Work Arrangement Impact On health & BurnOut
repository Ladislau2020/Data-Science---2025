    "# Create the pivot table with sum\n",
    "pivot_table = df.pivot_table(\n",
    "    index=\"Work_Life_Balance_Score\",\n",
    "    columns=\"Work_Arrangement\",\n",
    "    values=\"Issues_Count\",\n",
    "    aggfunc=\"sum\",\n",
    "    fill_value=0\n",
    ")\n",
    "\n",
    "# Convert to percentages\n",
    "percent_table = (pivot_table / pivot_table.values.sum()) * 100\n",
    "\n",
    "# Plot the heatmap\n",
    "plt.figure(figsize=(10, 6))\n",
    "sns.heatmap(percent_table, annot=True, cmap=\"Greens\", fmt=\".1f\")\n",
    "plt.title(\"% of Total Health Problems by Work_Life_Balance_Score and Work_Arrangement\")\n",
    "plt.xlabel(\"Work_Arrangement\")\n",
    "plt.ylabel(\"Work_Life_Balance_Score\")\n",
    "plt.xticks(rotation=45)\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
