    "\n",
    "# Plot the heatmap\n",
    "plt.figure(figsize=(8, 5))\n",
    "sns.heatmap(percent_table.values.reshape(-1, 1), \n",
    "            annot=True, cmap=\"Reds\", fmt=\".1f\", \n",
