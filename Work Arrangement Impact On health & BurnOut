   "source": [
    "def categorize_balance(score):\n",
    "    if score < 3:\n",
    "        return 'Poor'\n",
    "    elif score == 3:\n",
    "        return 'Good'\n",
    "    else:\n",
    "        return 'Excellent'\n",
    "\n",
    "df['Balance_Category'] = df['Work_Life_Balance_Score'].apply(categorize_balance)\n"
   ]
  },
