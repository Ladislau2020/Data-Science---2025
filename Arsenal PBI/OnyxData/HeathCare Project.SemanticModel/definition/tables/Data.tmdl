	measure BillingMOM =
			
			VAR Previous_Month = CALCULATE([Billing], DATEADD('DateT'[Date],-1, MONTH))
			VAR Current_Month = [Billing]
			RETURN
			IF(AND(Current_Month, Previous_Month), Current_Month / Previous_Month - 1)
		formatString: 0.00%;-0.00%;0.00%
		displayFolder: Variance
		lineageTag: 80a4d49e-6040-491d-9623-1a2863818589

	measure BillingYOY =
			
			VAR Previous_Year = CALCULATE([Billing], DATEADD('DateT'[Date],-1, YEAR))
			VAR Current_Year = [Billing]
			RETURN
