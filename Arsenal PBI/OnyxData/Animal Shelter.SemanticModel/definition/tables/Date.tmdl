	partition Date = calculated
		mode: import
		source =
				
				VAR Max_Date =
				    YEAR ( MAX ('Animal Shelter'[Outcome Date]))
				VAR Min_Date =
				    YEAR ( MIN ('Animal Shelter'[Intake Date]) )
				RETURN
				    ADDCOLUMNS (
				        FILTER (
				            CALENDARAUTO (),
				            YEAR ( [Date] ) >= Min_Date
				                && YEAR ( [Date] ) <= Max_Date
				        ),
				        "Year", YEAR ( [Date] ),
				        "QuarterNo", QUARTER([Date]),
				        "Quarter", FORMAT ( [Date], "\QQ" ),
				        "Month Number", MONTH ( [Date] ),
				        "Month", FORMAT ( [Date], "mmmm" ),
				        "WeekdayNo", WEEKDAY([Date]),
				        "Weekday", FORMAT([Date], "DDD")
				    )

	annotation PBI_Id = b24f9f36412f4b82963b65a83f6934f3

