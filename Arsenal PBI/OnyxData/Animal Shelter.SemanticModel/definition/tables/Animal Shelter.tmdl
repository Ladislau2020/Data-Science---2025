table 'Animal Shelter'
	lineageTag: ae8db875-3e30-4676-beb0-2093e8c07f4c

	measure 'Total registry' = COUNTROWS('Animal Shelter')
		formatString: 0
		displayFolder: General Metrics
		lineageTag: aa64c039-ebe4-41d3-8353-77df57b953eb

	measure Animals = DISTINCTCOUNT('Animal Shelter'[Animal ID])
		formatString: 0
		displayFolder: General Metrics
		lineageTag: f776d2ee-24b2-42b8-9381-05f1d7b85156

	measure Outcome =
			CALCULATE(
			    [Total registry],
			    USERELATIONSHIP('Animal Shelter'[Outcome Date], 'Date'[Date])
			)
		formatString: 0
		displayFolder: General Metrics
		lineageTag: 5fbb3212-1081-4b3c-9a46-2ee6fd7321fc

	measure 'live-release rate' =
			
			DIVIDE(
			    CALCULATE(
			        [Total registry],
			        'Animal Shelter'[outcome_is_alive] == TRUE()
			    ),
			    [Total registry]
			)
		formatString: 0.00%;-0.00%;0.00%
		displayFolder: General Metrics
		lineageTag: b753988a-41e4-46cd-8bfb-4a54f7f747e5

